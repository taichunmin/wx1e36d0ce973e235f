.van-collapse-item__title .van-cell__right-icon {
    transform: rotate(90deg);
    transition: transform .3s;
    transition: transform .3s,-webkit-transform .3s;
    -webkit-transition: -webkit-transform var(--collapse-item-transition-duration,.3s);
    transition: transform var(--collapse-item-transition-duration,.3s);
    transition: transform var(--collapse-item-transition-duration,.3s),-webkit-transform var(--collapse-item-transition-duration,.3s);
}

.van-collapse-item__title--expanded .van-cell__right-icon {
    transform: rotate(-90deg);
}

.van-collapse-item__title--disabled .van-cell,.van-collapse-item__title--disabled .van-cell__right-icon {
    color: #c8c9cc!important;
    color: var(--collapse-item-title-disabled-color,#c8c9cc)!important;
}

.van-collapse-item__title--disabled .van-cell--hover {
    background-color: #fff!important;
    background-color: var(--white,#fff)!important;
}

.van-collapse-item__wrapper {
    overflow: hidden;
}

.van-collapse-item__animation-box {
    height: 0;
}

.van-collapse-item__content {
    background-color: #fff;
    background-color: var(--collapse-item-content-background-color,#fff);
    color: #969799;
    color: var(--collapse-item-content-text-color,#969799);
    font-size: 26rpx;
    font-size: var(--collapse-item-content-font-size,26rpx);
    line-height: 1.5;
    line-height: var(--collapse-item-content-line-height,1.5);
    overflow: hidden;
    transition: all .3s ease-in-out;
}

.van-collapse-item__content_wrapper {
    padding: 30rpx;
    padding: var(--collapse-item-content-padding,30rpx);
}

page {
    background-color: #f4f4f4;
}

.h5-p {
    margin-top: 0;
}

.h5-p:last-child {
    margin-bottom: 0;
}

.van-collapse-item {
    background-color: #fff;
    border-radius: var(--border-radius-md);
    margin-bottom: 20rpx;
}

.van-collapse-item:last-child {
    border-width: 0;
}

.van-collapse-item .van-collapse-item__title {
    align-items: start;
    background-color: initial;
}

.van-collapse-item .van-collapse-item__title::after {
    border-width: 0;
}

.van-collapse-item .van-collapse-item__content {
    background-color: initial;
}

.van-collapse-item .van-collapse-item__content_wrapper {
    padding-top: 0;
}

.icon-cat {
    background-image: url(data:image/svg+xml;base64,);
    background-position: 50%;
    background-repeat: no-repeat;
    background-size: contain;
    height: 40rpx;
    position: relative;
    top: 2rpx;
    width: 40rpx;
}