.gogogo {
    display: flex;
    flex-direction: column;
    height: 100vh;
}

.gogogo-content {
    margin-top: 30%;
}

.gogogo-icon {
    display: block;
    width: 200rpx;
    height: 200rpx;
    background-position: center;
    background-repeat: no-repeat;
    background-size: contain;
    background-image: url(data:image/svg+xml;base64,);
    margin: auto;
}

.gogogo-action {
    margin-top: 15%;
}